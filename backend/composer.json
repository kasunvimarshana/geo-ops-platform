{
  "name": "kasunvimarshana/geo-ops-platform-backend",
  "description": "Laravel backend for GPS-based land measurement and agricultural field-service platform",
  "version": "1.0.0",
  "type": "project",
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0",
    "tymon/jwt-auth": "^2.0",
    "spatie/laravel-permission": "^6.0",
    "barryvdh/laravel-dompdf": "^2.0",
    "maatwebsite/excel": "^3.1",
    "predis/predis": "^2.0",
    "league/flysystem-aws-s3-v3": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.5",
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Seeders\\": "database/seeders/",
      "Database\\Factories\\": "database/factories/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "pint",
    "post-install-cmd": [
      "@php artisan key:generate --ansi",
      "@php artisan jwt:secret --force"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
